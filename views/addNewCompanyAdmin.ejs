       <script src="/javascripts/jquery.autotab.js"></script>
        <script src="/javascripts/jquery.cookie.js"></script>
<script type="text/javascript">
$(document).ready(function() {
                  
       
              $("#nav").height($("#section").outerHeight());
    var r=confirm("Load Last Record?");
    if (r == true) {
                  loadCookie();
                  }
    
                  
                  
                  
    $('#BillingContactPhone1').autotab({ target: '#BillingContactPhone2', format: 'numeric' });
    $('#BillingContactPhone2').autotab({ target: '#BillingContactPhone3', format: 'numeric', previous: '#BillingContactPhone1' });
    $('#BillingContactPhone3').autotab({ previous: '#BillingContactPhone2', format: 'numeric'});

                  
                  
                  
});

function setCookie(){
    $.cookie("1", $("#cp01").val(), { expires : 2 });
    $.cookie("2", $("#cp02").val(), { expires : 2 });
    $.cookie("3", $("#cp03").val(), { expires : 2 });
    $.cookie("5", $("#BillingState1").val(), { expires : 2 });
    $.cookie("4",$("#cp04").val(),{expires:2});
    
    $.cookie("BillingContactFirstName", $("#cp06").val(), { expires : 2 });
    $.cookie("BillingContactLastName", $("#cp07").val(), { expires : 2 });
    $.cookie("BillingContactEmail", $("#cp08").val(), { expires : 2 });
    $.cookie("Phone1", $("#BillingContactPhone1").val(), { expires : 2 });
    $.cookie("Phone2", $("#BillingContactPhone2").val(), { expires : 2 });
    $.cookie("Phone3", $("#BillingContactPhone3").val(), { expires : 2 });

}
function loadCookie(){
    $("#cp06").val($.cookie("BillingContactFirstName"));
    $("#cp07").val($.cookie("BillingContactLastName"));
    $("#cp08").val($.cookie("BillingContactEmail"));
    $("#BillingContactPhone1").val($.cookie("Phone1"));
    $("#BillingContactPhone2").val($.cookie("Phone2"));
    $("#BillingContactPhone3").val($.cookie("Phone3"));
    $("#cp01").val($.cookie("1"));
    $("#cp02").val($.cookie("2"));
    $("#cp03").val($.cookie("3"));
    $("#cp04").val($.cookie("4"));
    $("#BillingState1").val($.cookie("5"));
    
}
function submitCompany(){
    setCookie();
}
</script>
